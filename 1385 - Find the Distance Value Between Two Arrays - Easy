class Solution:
    def findTheDistanceValue(self, arr1: List[int], arr2: List[int], d: int) -> int:
        arr1.sort()
        arr2.sort()

        count = 0
        distance = 0
        for i in arr1:
            for j in arr2:
                if abs(i - j) > d:
                    count += 1
                if count == len(arr2):
                    distance += 1
            count = 0
        return distance
